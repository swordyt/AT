package com.yinting.core.datadriver;

import com.yinting.core.DataExchangeArea;
import com.yinting.core.TestCase;

public class ExcelDriver extends DataDriver {
	private Integer i = 0;
	private int position = 0;
	private int total = 0;

	public ExcelDriver() {
		System.out.println(Thread.currentThread().getName());
		total =TestCase.local.length;
	}

	public boolean hasNext() {
		// TODO Auto-generated method stub
		return position < total;
	}

	public Object[] next() {
		return new Object[] { TestCase.local[position++] };
	}

	@Override
	public int getType() {
		// TODO Auto-generated method stub
		return 0;
	}

}
